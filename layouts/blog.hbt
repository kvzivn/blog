{{> header}}

<div class="Archive">
  <ul class="Archive-list">
    {{#each collections.posts}}
    <li class="Archive-post">
      <a href="{{this.path}}">
        <span class="Archive-postTitle">{{this.title}}</span>
        <span class="Archive-postDate">{{formatDate this.date day="numeric" month="long" year="numeric"}}</span>
      </a>
    </li>
    {{/each}}
  </ul>
</div>

{{> footer}}